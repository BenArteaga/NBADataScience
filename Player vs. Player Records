   "source": [
    "#class to represent a pair of players and their record against one another\n",
    "class playerPair:\n",
    "    def __init__(self, winPerc, wins, losses, playerA, playerB):\n",
    "        self.winPerc = winPerc\n",
    "        self.wins = wins\n",
    "        self.losses = losses\n",
    "        self.playerA = playerA\n",
    "        self.plsyerB = playerB\n",
    "        return\n",
    "    #custom comparator so that the pq is a min pq based on winPerc\n",
    "    def __lt__(self, other):\n",
    "        return self.winPerc < other.winPerc\n",
    "\n",
    "#function to get two players records against eachother in the format of playerA's winning percentage\n",
    "def getRecord(playerA, playerB):\n",
    "    from nba_api.stats.static import players\n",
    "\n",
    "    #get two players from the players endpoint\n",
    "    player_dict = players.get_players()\n",
    "    player1 = [player for player in player_dict if player['full_name'] == playerA][0]\n",
    "    player1_id = player1['id']\n",
    "\n",
    "    player2 = [player for player in player_dict if player['full_name'] == playerB][0]\n",
    "    player2_id = player2['id']\n",
    "\n",
    "    #Get game logs for each player\n",
    "    from nba_api.stats.library.parameters import SeasonAll\n",
    "    from nba_api.stats.endpoints import playergamelog\n",
    "    import pandas as pd\n",
    "\n",
    "    gamelog_player1 = playergamelog.PlayerGameLog(player_id=player1_id, season = SeasonAll.all)\n",
    "    df_player1 = gamelog_player1.get_data_frames()\n",
    "    player1_games = gamelog_player1.get_normalized_dict()['PlayerGameLog']\n",
    "\n",
    "    gamelog_player2 = playergamelog.PlayerGameLog(player_id = player2_id, season = SeasonAll.all)\n",
    "    player2_games = gamelog_player2.get_normalized_dict()['PlayerGameLog']\n",
    "    df_player2 = gamelog_player2.get_data_frames()\n",
    "\n",
    "    #Calculate record of head to head matchups of the two players\n",
    "    player1Wins = 0\n",
    "    player2Wins = 0\n",
    "    for game in player1_games:\n",
    "        for game2 in player2_games:\n",
    "            if game2['GAME_DATE'] == game['GAME_DATE']:\n",
    "                if game2['MATCHUP'][-3:] == game['MATCHUP'][:3]:\n",
    "                    if game2['WL'] == 'W':\n",
    "                        player2Wins = player2Wins + 1\n",
    "                    else:\n",
    "                        player1Wins = player1Wins + 1\n",
    "\n",
    "#     print(\"{} is {}-{} vs. {}.\".format(playerA, player1Wins, player2Wins, playerB))\n",
    "    if(player1Wins + player2Wins > 0):\n",
    "        winPerc = player1Wins / (player1Wins + player2Wins)\n",
    "    else:\n",
    "        winPerc = 0\n",
    "    pair = playerPair(winPerc, player1Wins, player2Wins, player1, player2)\n",
    "    return pair\n",
    "\n",
    "test = getRecord('LeBron James', 'Kemba Walker')\n",
    "print(test.winPerc)       \n",
    "\n",
    "def getMostLopsided(num):\n",
    "    #initialize pq to values with winning percentage as 0\n",
    "    import queue as Q\n",
    "    dummy = playerPair(0, 0, 0, 'Kwame', 'Darko')\n",
    "    pq = Q.PriorityQueue()\n",
    "    for i in range(0,num):\n",
    "        pq.put(dummy)\n",
    "        \n",
    "    #try every combination of players \n",
    "    from nba_api.stats.static import players\n",
    "    player_dict = players.get_players()\n",
    "    for i in range(0, len(player_dict)):\n",
    "        time.sleep(3)\n",
    "        for j in range(i + 1, len(player_dict)):\n",
    "            newPair = getRecord(player_dict[i]['full_name'], player_dict[j]['full_name'])\n",
    "            oldPair = pq.get()\n",
    "            if((newPair.winPerc > oldPair.winPerc) and ((newPair.wins + newPair.losses) >= 10)):\n",
    "                pq.put(newPair)\n",
    "            else:\n",
    "                pq.put(oldPair)\n",
    "    \n",
    "    #print final list\n",
    "    while(pq.empty() == false):\n",
    "        currPair = pq.get()\n",
    "        print(\"{} is {}-{} vs. {}.\".format(currPair.playerA, currPair.wins, currPair.losses, currPair.playerB))\n",
    "\n",
    "getMostLopsided(10)\n",
    "    \n",
    "    "
   ]
